<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OrganiserV2.Pages"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="OrganiserV2.Pages.ProjectDetails"
             mc:Ignorable="d" 
             d:DesignHeight="680" d:DesignWidth="1095">
    <Grid Background="#FFEAEAEA">
        <Label x:Name="projectTitle" HorizontalAlignment="Left" VerticalAlignment="Top" Height="42" Width="1095" FontSize="22" Background="#FF383838"/>
        <Label x:Name="stateText" Content="State:" HorizontalAlignment="Left" Margin="6,44,0,0" VerticalAlignment="Top" Width="1075" Foreground="#DD000000" Height="36" FontSize="20"/>
        <TextBlock Margin="10,116,14,537" FontSize="20" FontWeight="Bold" TextDecorations="{x:Null}">
            <Hyperlink x:Name="adoLink" NavigateUri="" Click="adoLink_Click">ADO Link</Hyperlink>
        </TextBlock>
        <Label x:Name="dateCreatedText" Content="Date Created:" HorizontalAlignment="Left" Margin="6,80,0,0" VerticalAlignment="Top" Width="1075" Foreground="#DD000000" Height="36" FontSize="20"/>
    </Grid>
</UserControl>
