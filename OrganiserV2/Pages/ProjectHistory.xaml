<UserControl x:Class="OrganiserV2.Pages.ProjectHistory"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OrganiserV2.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="680" d:DesignWidth="1095">
    <Grid>
        <Label Content="Project History" HorizontalAlignment="Left" VerticalAlignment="Top" Height="42" Width="1095" FontSize="22" Background="#FF383838" Margin="0,0,-270,0"/>
        <ListBox x:Name="oldProjectsList" HorizontalAlignment="Left" Height="614" Margin="0,66,0,0" VerticalAlignment="Top" Width="1095"/>
        <TextBox x:Name="projectSearch" HorizontalAlignment="Left" Height="24" Margin="0,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="1095" MaxHeight="680" Background="#FF5F5F5F" Foreground="White" KeyUp="projectSearch_KeyUp"/>

    </Grid>
</UserControl>
